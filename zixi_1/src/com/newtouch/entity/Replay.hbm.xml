<?xml version="1.0"?>


<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    
    <hibernate-mapping>
    
    <class name="com.newtouch.entity.Replay">
    <id name="Id">
    <generator class="native"/>
    </id>
    <property name="topicId"/>
    <property name="content"/>
    <property name="date"/>
    <property name="d_delete"/>
   <many-to-one name="manageruser" lazy="false" class="com.newtouch.entity.User"></many-to-one>
    </class>
    
    </hibernate-mapping>